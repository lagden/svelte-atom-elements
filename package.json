{
  "name": "@tadashi/svelte-atom-elements",
  "version": "1.9.0",
  "repository": "github:lagden/svelte-atom-elements",
  "description": "Svelte components",
  "keywords": [
    "svelte",
    "elements",
    "atom",
    "component"
  ],
  "svelte": "src/elements.js",
  "main": "src/elements.js",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "prettier": "prettier -c --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "esbuild": "./esbuild.config.js",
    "build": "run-s test rm esbuild",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "test": "jest test --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@testing-library/svelte": "3.1.1",
    "babel-jest": "28.0.2",
    "esbuild": "0.14.38",
    "esbuild-svelte": "0.7.0",
    "eslint": "8.14.0",
    "eslint-config-xo": "0.40.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-svelte3": "3.4.1",
    "eslint-plugin-unicorn": "42.0.0",
    "jest": "28.0.2",
    "jest-environment-jsdom": "28.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "2.7.0",
    "svelte": "3.47.0",
    "svelte-jester": "2.3.2",
    "timekeeper": "2.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
